### Setup experimental parameters
runs = 2
#output-directory = ../GAExperiments/Debs4Bit/OneBitMutStringSize40/pop500gen501runs1000/ssmNONEregtourneysize7

output-directory = ../GAExperiments/abcdefg
output-file-prefix = run
output-file-suffix = .txt


### Setup required parameters

problem-class = org.spiderland.Psh.BitstringGA.Decimation5
individual-class = org.spiderland.Psh.BitstringGA.BitstringGAIndividual

population-size = 500
max-generations = 501

tournament-size = 7     #Change this back later!!!!!!!!!!!!
mutation-percent = 95
crossover-percent = 0


### Setup GA parameters
bitstring-size = 40

#Options: one-bit, per-bit
ga-mutation-mode = one-bit
#ga-mutation-percent = 8


### Setup Optional Parameters

#options: none, decimation, truncation
survival-selection-mode = none
survival-ratio = 10
survival-tournament-size = 2
